{
	"name": "nextjs-component-generator",
	"displayName": "nextjs-component-generator",
	"description": "generator new component with template",
	"version": "0.0.4",
	"publisher": "antn9x",
	"repository": "https://github.com/antn9x/nextjs-component-generator",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:nextjs-component-generator.createNewComponent",
		"onCommand:nextjs-component-generator.createNewPage"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nextjs-component-generator.createNewComponent",
				"title": "Next-JS components generator: Create new component"
			},
			{
				"command": "nextjs-component-generator.createNewPage",
				"title": "Next-JS components generator: Create new page"
			}
		]
	},
	"scripts": {
		"deploy": "vsce package",
		"vscode:prepublish": "yarn run compile",
		"compile": "webpack",
		"package": "webpack --mode production --devtool hidden-source-map",
		"watch": "webpack --watch",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/lodash": "^4.14.181",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/pluralize": "^0.0.29",
		"@types/vscode": "^1.66.0",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"lodash": "^4.17.21",
		"mocha": "^9.2.2",
		"ts-loader": "^9.2.8",
		"typescript": "^4.5.5",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"pluralize": "^8.0.0"
	}
}
